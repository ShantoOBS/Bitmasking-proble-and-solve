#include<bits/stdc++.h>
using namespace std;
void print(int n){
    
    for(int i=31;i>=0;i--){
        cout<<((n>>i)&1);
    }
    cout<<endl;
}
int main()
{
   
  int a=59;
  int i=4;
  
  int b=(a&(~((1<<i+1)-1)));
   //0000111011 --59
   //1111100000----need this number  (~((1<<i+1)-1))
  //0000011111-- ((1<<i+1)-1)
  //0000100000 --(1<<i+1)
  
  
  print(b);
   
   
   
}
