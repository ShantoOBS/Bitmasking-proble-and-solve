#include<bits/stdc++.h>
using namespace std;
void print(int n){
    
    for(int i=31;i>=0;i--){
        cout<<((n>>i)&1);
    }
    cout<<endl;
}
int main()
{
  int n;
  cin>>n;
    // 1 right shift means multipy by 2 
  cout<<(n>>1)<<endl;
  
  //1 left shilf means divide by 2;
  cout<<(n<<1)<<endl;
 
   
   
   
}
